@model IEnumerable<Employee>
@{
    ViewData["Title"] = "Home Page";
}


<table class="table table-striped">
    <tr>
        <th>Employee Id</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>City</th>
        <th>Mobile</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.EmployeeID</td>
            <td>@item.FirstName</td>
            <td>@item.LastName</td>
            <td>@item.Email</td>
            <td>@item.City</td>
            <td>@item.Mobile</td>
        </tr>

    }

</table>
